{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "startCommand": "gunicorn RecipeNet.wsgi:application --bind 0.0.0.0:8000",
        "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ]
}